{
  "name": "blog",
  "description": "egineer blog of t-yng",
  "version": "2.0",
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "export": "next export",
    "dev": "next dev",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "start": "next start",
    "bundle-analyze": "ANALYZE=true next build",
    "test": "vitest run",
    "test:coverage": "yarn test --coverage",
    "test:e2e": "playwright test",
    "lint:all": "eslint --ext ts,tsx ./ && yarn lint:html",
    "lint:html": "markuplint -p \"./src/**/*.tsx\"",
    "lint:fix": "eslint --fix --ext ts,tsx ./",
    "type-check": "tsc",
    "prepare": "husky install"
  },
  "engines": {
    "node": "18.x"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings=0"
    ],
    "*.{tsx}": [
      "markuplint -p"
    ]
  },
  "dependencies": {
    "@highlight-run/next": "4.1.0",
    "@highlight-run/react": "3.2.1",
    "date-fns": "2.30.0",
    "gray-matter": "4.0.3",
    "highlight.run": "7.4.3",
    "next": "13.4.19",
    "prismjs": "^1.25.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@markuplint/jsx-parser": "3.9.1",
    "@markuplint/react-spec": "3.10.1",
    "@next/bundle-analyzer": "13.4.19",
    "@playwright/test": "1.37.1",
    "@swc/core": "^1.2.117",
    "@testing-library/jest-dom": "6.1.3",
    "@testing-library/react": "14.0.0",
    "@types/cpx": "1.5.3",
    "@types/gtag.js": "0.0.13",
    "@types/markdown-it": "13.0.1",
    "@types/prismjs": "^1.16.6",
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@typescript-eslint/parser": "6.7.0",
    "@vitejs/plugin-react": "4.0.4",
    "@vitest/coverage-v8": "0.34.4",
    "cheerio": "1.0.0-rc.12",
    "cpx": "1.5.0",
    "eslint": "8.49.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "hygen": "6.2.11",
    "identity-obj-proxy": "3.0.0",
    "image-size": "^1.0.0",
    "jsdom": "22.1.0",
    "linaria": "4.5.4",
    "lint-staged": "14.0.1",
    "markdown-it": "13.0.1",
    "markuplint": "3.12.1",
    "next-linaria": "0.11.0",
    "prettier": "3.0.3",
    "ts-mockito": "2.6.1",
    "typescript": "5.2.2",
    "url-join": "5.0.0",
    "vitest": "0.34.4"
  }
}
