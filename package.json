{
  "name": "gatsby-starter-hello-world",
  "description": "egineer blog of t-yng",
  "version": "2.0",
  "license": "MIT",
  "scripts": {
    "build": "next build && next export",
    "develop": "next dev",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "start": "next start",
    "bundle-analyze": "ANALYZE=true next build",
