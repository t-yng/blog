    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "hygen": "^6.0.4",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "postcss": "^8.3.0",
    "prettier": "2.0.5",
    "react-test-renderer": "^17.0.1",
    "tailwindcss": "^2.1.2",
    "ts-jest": "^26.4.4",
    "ts-mockito": "^2.6.1",
    "typescript": "^4.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
