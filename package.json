{
  "name": "blog",
  "description": "egineer blog of t-yng",
  "version": "2.0",
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "export": "next export",
    "dev": "next dev",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "start": "next start",
    "bundle-analyze": "ANALYZE=true next build",
    "test": "vitest run",
    "test:coverage": "yarn test --coverage",
    "lint": "eslint --ext ts,tsx ./ && yarn lint:html",
    "lint:html": "markuplint -p \"./src/**/*.tsx\"",
    "type-check": "tsc",
    "prepare": "husky install"
  },
  "engines": {
    "node": "16.x"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0"
    ],
    "*.{tsx}": [
      "markuplint -p"
    ]
  },
  "dependencies": {
    "@vanilla-extract/css": "1.9.2",
    "@vanilla-extract/next-plugin": "2.1.1",
    "date-fns": "2.29.3",
    "gray-matter": "4.0.3",
    "next": "13.4.1",
    "prismjs": "^1.25.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@markuplint/jsx-parser": "3.3.1",
    "@markuplint/react-spec": "3.3.0",
    "@next/bundle-analyzer": "13.2.4",
    "@swc/core": "^1.2.117",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@types/gtag.js": "0.0.12",
    "@types/markdown-it": "12.2.3",
    "@types/prismjs": "^1.16.6",
    "@typescript-eslint/eslint-plugin": "5.59.6",
    "@typescript-eslint/parser": "5.59.6",
    "@vanilla-extract/vite-plugin": "3.8.0",
    "@vitejs/plugin-react": "4.0.0",
    "@vitest/coverage-c8": "0.29.2",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.3",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "hygen": "6.2.11",
    "identity-obj-proxy": "3.0.0",
    "image-size": "^1.0.0",
    "jsdom": "22.0.0",
    "lint-staged": "13.1.2",
    "markdown-it": "13.0.1",
    "markuplint": "3.3.1",
    "prettier": "2.8.4",
    "ts-mockito": "2.6.1",
    "typescript": "5.0.4",
    "vitest": "0.29.2"
  }
}
