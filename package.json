    "gray-matter": "^4.0.2",
    "lazysizes": "^5.3.2",
    "next": "^10.0.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-markdown": "^5.0.3",
    "react-syntax-highlighter": "^15.4.3"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^10.0.5",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@types/gtag.js": "^0.0.4",
    "@types/react-helmet": "^6.0.0",
    "@types/react-syntax-highlighter": "^13.5.0",
